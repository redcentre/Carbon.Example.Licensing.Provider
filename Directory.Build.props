<Project>
  <PropertyGroup>
    <Version>9.2.4</Version>
    <TargetFrameworks>net8.0</TargetFrameworks>
    <IncludeSourceRevisionInInformationalVersion>false</IncludeSourceRevisionInInformationalVersion>
    <Nullable>enable</Nullable>
    <NoWarn>CS8618</NoWarn>
    <Product>Carbon</Product>
    <Authors>Red Centre Software</Authors>
    <Company>Red Centre Software</Company>
    <Copyright>Copyright Â© 2020-2025 Red Centre Software</Copyright>
    <ProduceReferenceAssembly>False</ProduceReferenceAssembly>
    <PackageTags>redcentre;Red Centre Software;cross tabulation;crosstab</PackageTags>
    <IsPublishable>false</IsPublishable>
  </PropertyGroup>
  <ItemGroup>
    <AssemblyAttribute Include="System.Reflection.AssemblyMetadataAttribute">
      <_Parameter1>BuildMachine</_Parameter1>
      <_Parameter2>$(COMPUTERNAME)</_Parameter2>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Reflection.AssemblyMetadataAttribute">
      <_Parameter1>BuildTime</_Parameter1>
      <_Parameter2>$([System.DateTime]::Now.ToString("yyyy-MM-dd HH:mm:ss K"))</_Parameter2>
    </AssemblyAttribute>
  </ItemGroup>
</Project>
